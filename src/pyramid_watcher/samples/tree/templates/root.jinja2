{% extends "layout.jinja2" %}

{% block content %}
    <h1 class="title">
        Root: {{ context.title }}
    </h1>
    <h2>Children</h2>
    <ul>
        {% for child in children %}
            <li>
                <a href="{{ request.resource_url(child) }}">{{ child.title }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock content %}

